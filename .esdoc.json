{
  "source": "./",
  "index": "./README-esdoc.md",
  "destination": "./docs",
  "excludes": ["node_modules", "docs", ".git", "tests", "utils", "coverage"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "manual": {
          "files": [
            "./documents/node/appConfiguration.md",
            "./documents/node/environmentUtils.md",
            "./documents/node/errorHandler.md",
            "./documents/node/logger.md",
            "./documents/node/packageInfo.md",
            "./documents/node/pathUtils.md",
            "./documents/node/rootRequire.md"
          ]
        }
      }
    },
    {"name": "esdoc-node"}
  ]
}
